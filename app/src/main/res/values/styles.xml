        <item name="android:paddingRight">@dimen/paddingswitch2_right</item>
        <item name="android:paddingLeft">@dimen/paddingswitch2_left</item>
        <item name="android:paddingBottom">@dimen/paddingswitch2_bottom</item>
    </style>
</resources>
