    <style name="textView2">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">115dp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginLeft">@dimen/margin_textview_left</item>
        <item name="android:layout_marginTop">@dimen/margin_textview_top</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingStart">@dimen/paddingtextview_start</item>
        <item name="android:paddingTop">@dimen/paddingtextview_top</item>
        <item name="android:textAllCaps">false</item>
      <!--  <item name="android:fontFamily">@font/neuropol</item> -->
    </style>
    <style name="textView3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
