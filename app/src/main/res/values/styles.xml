        <item name="android:layout_marginLeft">@dimen/margin_textview2_left</item>
        <item name="android:layout_marginStart">@dimen/margin_textview2_start</item>
        <item name="android:layout_marginBottom">@dimen/margin_textview2_bottom</item>
